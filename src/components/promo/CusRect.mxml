<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 preinitialize="group1_preinitializeHandler(event)">
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			
			[Bindable] public var radX1:Number = 0;
			[Bindable] public var radY1:Number = 0;
			[Bindable] public var radX2:Number = 0;
			[Bindable] public var radY2:Number = 0;
			[Bindable] public var rectColor:uint = 0xFFFFFF;
			
			protected function group1_preinitializeHandler(event:FlexEvent):void
			{
				// TODO Auto-generated method stub
				
			}
			
		]]>
	</fx:Script>
	<s:Rect width="100%" height="100%" alpha="0.85" 
			topLeftRadiusX="{radX1}"
			bottomLeftRadiusY="{radY1}"
			topRightRadiusX="{radX2}" 
			bottomRightRadiusX="{radY2}">
		<s:fill>
			<s:SolidColor color="{rectColor}"/>
		</s:fill>
	</s:Rect>
</s:Group>
